#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int X;
        scanf("%d", &X);

        if (X % 5 != 0) {
            printf("-1\n");
        } else {
            int coins = (X / 10) + ((X % 10) / 5);
            printf("%d\n", coins);
        }
    }

    return 0;
}
